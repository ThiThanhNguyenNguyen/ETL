  <DataSets>
    <DataSet Name="AverageListingPrice">
      <Query>
        <DataSourceName>DataMart</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pMonth">
            <Value>=Parameters!pMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pYear">
            <Value>=Parameters!pYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  
SUBSTRING(CAST([DateKey] AS VARCHAR(10)),7,2) AS SingleDay,
	[DateKey],
	ROUND(AVG([Price]),2) AS AvgListingPrice,
                COUNT ([DateKey]) AS TotalItem
FROM [DataMart].[dbo].[factRealEstateForSale]  with(nolock)
WHERE
	1=1
	AND [PRICE] IS NOT NULL 
	AND SUBSTRING(CAST([DateKey] AS VARCHAR(100)),5,2) IN (@pMonth)
	AND SUBSTRING(CAST([DateKey] AS VARCHAR(100)),1,4) IN (@pYear)
GROUP BY [DateKey]
ORDER BY [DateKey]</CommandText>
      </Query>
      <Fields>
        <Field Name="SingleDay">
          <DataField>SingleDay</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateKey">
          <DataField>DateKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AvgListingPrice">
          <DataField>AvgListingPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalItem">
          <DataField>TotalItem</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
